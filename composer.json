{
    "name": "lioneagle/lioneagle-paginator",
    "description": "A simple package to allow Laravels Paginator to get the page size and page number for the URL",
    "keywords": [
        "lioneagle",
        "laravel",
        "lioneagle-paginator"
    ],
    "homepage": "https://github.com/lioneagle/lioneagle-paginator",
    "license": "MIT",
    "authors": [
        {
            "name": "James Mooring",
            "email": "james@lioneagle.solutions",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Lioneagle\\LioneaglePaginator\\": "src",
            "Lioneagle\\LioneaglePaginator\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lioneagle\\LioneaglePaginator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lioneagle\\LioneaglePaginator\\LioneaglePaginatorServiceProvider"
            ],
            "aliases": {
                "LioneaglePaginator": "Lioneagle\\LioneaglePaginator\\LioneaglePaginatorFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
