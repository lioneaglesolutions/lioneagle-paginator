{
    "name": "lioneagle/lioneagle-paginator",
    "description": "A simple package to allow Laravels Paginator to get the page size and page number from the URL query parameters.",
    "keywords": [
        "lioneagle",
        "laravel",
        "lioneagle-paginator"
    ],
    "homepage": "https://github.com/lioneagle/lioneagle-paginator",
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Lioneagle\\LioneaglePaginator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lioneagle\\LioneaglePaginator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lioneagle\\LioneaglePaginator\\LioneaglePaginatorServiceProvider"
            ],
            "aliases": {
                "LioneaglePaginator": "Lioneagle\\LioneaglePaginator\\LioneaglePaginatorFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}